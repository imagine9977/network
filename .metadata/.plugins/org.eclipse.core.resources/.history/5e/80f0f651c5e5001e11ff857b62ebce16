package sec1;

import java.net.URL;
import java.net.URLConnection;

public class URLConnectionExam {
	public static void main(String[] args) {
		try {
			URL kbs = new URL("https://program.kbs.co.kr/2tv/drama/nothinguncovered/pc/index.html");
			try {
				URLConnection kbsCon = kbs.openConnection();
				System.out.println(kbsCon.getContentType());
			} catch (Exception e) {
				// TODO: handle exception
			}
		} catch (Exception e) {
			// TODO: handle exception
		}
	}
}
